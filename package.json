{
  "name": "@nuxtjs/webpackmonitor",
  "version": "0.0.3",
  "description": "Monitor Nuxt webpack optimization metrics through the development process",
  "license": "MIT",
  "contributors": [
    {
      "name": "Pooya Parsa <pooya@pi0.ir>"
    }
  ],
  "main": "lib/module.js",
